#include <stdio.h>

int main()
{
    int days , years , weeks;
    
    printf("Enter number of the days:");
    scanf("%d",&days );
    
    years= days/365;
    weeks= (days - years*365)/7;
    days= days - (years*365 + weeks*7);
    
    printf(" years=%d, weeks=%d, days=%d ",years , weeks, days);
    
    return 0;
}
